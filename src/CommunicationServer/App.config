<?xml version="1.0" encoding="utf-8"?>

<configuration>
  <configSections>
    <section name="serverConfig" type="_15pl04.Ucc.Commons.Config.ServerConfigurationSection, Ucc.Commons" />
  </configSections>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>

  <serverConfig address="127.0.0.1" port="12345" timeout="5" isBackup="false">
    <masterServer address="127.0.0.1" port="12345" />
  </serverConfig>

  <system.diagnostics>

    <switches>
      <add name="off" value="Off" />
      <add name="crit" value="Critival" />
      <add name="error" value="Error" />
      <add name="warn" value="Warning" />
      <add name="info" value="Information" />
      <add name="verb" value="Verbose" />
      <add name="activityTracing" value="ActivityTracing" />
      <add name="all" value="All" />
    </switches>

    <sharedListeners>
      <add name="consoleListener"
           type="_15pl04.Ucc.Commons.Logging.CustomConsoleTraceListener, Ucc.Commons"
           printDate="true"
           printSource="" />
    </sharedListeners>

    <sources>
      <source name="ComponentOverseer" switchName="all">
        <listeners>
          <add name="consoleListener" />
        </listeners>
      </source>
      <source name="MessageProcessor" switchName="all">
        <listeners>
          <add name="consoleListener" />
        </listeners>
      </source>
      <source name="TcpServer" switchName="all">
        <listeners>
          <add name="consoleListener" />
        </listeners>
      </source>
      <source name="WorkManager" switchName="all">
        <listeners>
          <add name="consoleListener" />
        </listeners>
      </source>
    </sources>

  </system.diagnostics>

</configuration>
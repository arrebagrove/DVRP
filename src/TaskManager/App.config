<?xml version="1.0" encoding="utf-8"?>

<configuration>
  <configSections>
    <section name="componentConfig" type="_15pl04.Ucc.Commons.Config.ComponentConfigurationSection, Ucc.Commons" />
  </configSections>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>

  <componentConfig taskSolversPath="/">
    <primaryServer address="127.0.0.1" port="12345" />
  </componentConfig>

  <system.diagnostics>

    <switches>
      <add name="off" value="Off" />
      <add name="crit" value="Critival" />
      <add name="error" value="Error" />
      <add name="warn" value="Warning" />
      <add name="info" value="Information" />
      <add name="verb" value="Verbose" />
      <add name="activityTracing" value="ActivityTracing" />
      <add name="all" value="All" />
    </switches>

    <sharedListeners>
      <add name="consoleListener"
           type="_15pl04.Ucc.Commons.Logging.CustomConsoleTraceListener, Ucc.Commons"
           printDate="true"
           printSource="" />
    </sharedListeners>

    <sources>
      <source name="TaskManager" switchName="all">
        <listeners>
          <add name="consoleListener" />
        </listeners>
      </source>
    </sources>

  </system.diagnostics>

</configuration>